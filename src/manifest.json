{
  "name": "IRMA Gmail",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["contentScript.bundle.js"]
    }
  ],

  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["https://ihub.ru.nl/irma/*", "https://main.irmaseal-pkg.ihub.ru.nl/v2/parameters"]
}
