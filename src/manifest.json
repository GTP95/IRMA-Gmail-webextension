{
  "name": "IRMA Gmail",
  "options_page": "options.html",
  "background": {
    "scripts": ["backgroundScript.bundle.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["contentScript.bundle.js"]
    }
  ],

  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
